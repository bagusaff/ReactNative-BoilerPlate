{
  "name": "onepassproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "extract-all": "npm run extract -- 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' ",
    "compile-all": "npm run compile -- src/lang/en.json --ast --out-file src/compiled-lang/en.json && npm run compile -- src/lang/zh.json --ast --out-file src/compiled-lang/zh.json && npm run compile -- src/lang/ms.json --ast --out-file src/compiled-lang/ms.json && npm run compile -- src/lang/id.json --ast --out-file src/compiled-lang/id.json"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/drawer": "^6.7.0",
    "@react-navigation/elements": "^1.3.30",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "moment": "^2.30.1",
    "react": "18.3.1",
    "react-intl": "^6.6.8",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "^15.4.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-vision-camera": "^4.4.2",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@babel/runtime": "^7.24.8",
    "@formatjs/cli": "^6.2.12",
    "@react-native/eslint-config": "^0.74.85",
    "@react-native/metro-config": "^0.74.85",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
